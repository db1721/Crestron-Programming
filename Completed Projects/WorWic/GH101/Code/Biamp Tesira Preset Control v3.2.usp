PresetComponent component;

/*******************************************************************************************
  Functions
  (Add any additional functions here)
  Note:  Functions must be physically placed before the location in
         the code that calls them.
*******************************************************************************************/
EVENTHANDLER OnInitializeChange(PresetComponent sender, UInt16EventArgs args)
{
	Is_Initialized = args.Payload;
}

/*******************************************************************************************
  Event Handlers
  (Uncomment and declare additional event handlers as needed)
*******************************************************************************************/

PUSH Recall_Preset
{
	component.RecallPreset(Preset_Name_Number);
}

/*******************************************************************************************
  Main()
  Uncomment and place one-time startup code here
  (This code will get called when the system starts up)
*******************************************************************************************/

Function Main()
{
    WaitForInitializationComplete(); 
	REGISTEREVENT(component, OnInitializeChange, OnInitializeChange);  
	
	component.Configure(Command_Processor_ID);
}

