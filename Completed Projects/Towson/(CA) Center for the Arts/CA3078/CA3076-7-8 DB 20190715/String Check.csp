#DEFINE_CONSTANT	MAXSIZE			20

#DEFAULT_NONVOLATILE
#ENABLE_STACK_CHECKING
#ENABLE_TRACE

STRING_INPUT     SerialIn$[MAXSIZE];
DIGITAL_OUTPUT   Is_Null;
DIGITAL_OUTPUT   Is_String;

CHANGE SerialIn$
{
	if(len(SerialIn$) > 0)
		Is_Null = 0;
	else
		Is_Null = 1;
			
	Is_String = ! Is_Null;
}

Function Main()
{
	Is_Null = 1;
	Is_String = 0;
}


